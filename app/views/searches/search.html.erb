      <h2>Books search for "test"</h2>
<% if @range == "User" %>
<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th></th>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <tr>
  <% @users.each do |user| %>
  <td><%= image_tag user.get_profile_image, size: '50x50' %></td>
  <td><%= user.name %></td>
  </tr>
  <% end %>
  </tbody>
  </table>

  <% else %>
  <table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th></th>
      <th>Title</th>
      <th>Opinion</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <tr>
  <% @books.each do |book| %>
  <td><%= image_tag book.user.get_profile_image, size: '50x50' %></td>
  <td><%= book.title %></td>
  <td><%= book.body %></td>
  </tr>
  <% end %>
  <% end %>
  </tbody>
  </table>
  </div>
  </div>
